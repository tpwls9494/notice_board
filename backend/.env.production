# Production Environment Variables
# IMPORTANT: Change these values for production!

DATABASE_URL=postgresql://postgres:YOUR_SECURE_PASSWORD@postgres:5432/company_board
REDIS_URL=redis://redis:6379/0

# Generate a new secret key with: openssl rand -hex 32
SECRET_KEY=CHANGE_THIS_TO_A_SECURE_RANDOM_STRING_USE_openssl_rand_hex_32
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=1440

# Database credentials
POSTGRES_USER=postgres
POSTGRES_PASSWORD=YOUR_SECURE_PASSWORD
POSTGRES_DB=company_board

# CORS - Add your EC2 IP or domain
CORS_ORIGINS=http://YOUR_EC2_IP_OR_DOMAIN,https://YOUR_EC2_IP_OR_DOMAIN
